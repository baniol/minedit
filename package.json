{
  "name": "minedit",
  "version": "0.1.1",
  "description": "simple markdown to html page generator",
  "bin": "bin/minedit",
  "main": "index.js",
  "scripts": {
    "start": "node --harmony index.js",
    "copy": "node index.js copy",
    "clear": "node index.js clear",
    "map": "node index.js map",
    "test": "./node_modules/mocha/bin/mocha test/*.js --reporter spec",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- test/*.js -R spec"
  },
  "private": "true",
  "author": "Marcin Baniowski",
  "license": "MIT",
  "dependencies": {
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "recursive-readdir": "^1.3.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4"
  }
}
